{
  "name": "mock-server",
  "version": "0.1",
  "description": "Build and Serve Test Data",
  "main": "target/index.js",
  "scripts": {
    "watch": "nodemon -w sandbox -w lib --exec \"babel-node sandbox --presets env\"",
    "build": "babel src -s -D -d target --presets env",
    "start": "node target",
    "prettier": "prettier --write --single-quote --print-width 100 \"{lib, src, sandbox}/**/*.js\""
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "lodash-es": "^4.17.10",
    "morgan": "^1.8.0",
    "nodemon": "^1.9.2"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.0",
    "faker": "^4.1.0",
    "prettier": "1.12.1"
  }
}
